    "typescript": "^4.7.4"
  },
  "dependencies": {
    "@prisma/client": "^4.0.0",
    "apollo-server": "^3.9.0",
    "graphql": "^16.5.0",
    "graphql-scalars": "^1.17.0",
    "nexus": "^1.3.0",
    "nodemon": "^2.0.19"
  },
